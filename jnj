int Sum(int a, int b) {
	return a + b;
}
#pragma once
int Sum(int, int);
#include <iostream>
#include "..\StaticLib\Header.h"

using namespace std;

int main() {
	int a = 10;
	int b = 5;
	cout << Sum(a, b);

}
extern "C" __declspec(dllexport) int Sum(int a, int b) {
	return a + b;
}
#pragma once
extern "C" __declspec(dllexport) int Sum(int, int);

#include <iostream>
#include <Windows.h>

using namespace std;

int main() {
	HINSTANCE load = LoadLibrary(L"DynamicLib.dll");
	typedef int (*sum) (int, int);

	sum Sum;
	Sum = (sum)GetProcAddress(load, "Sum");
	int a = 10;
	int b = 50;
	cout << Sum(a, b);
	FreeLibrary(load);

}
